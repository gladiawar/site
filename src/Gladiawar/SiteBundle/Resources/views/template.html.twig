<body>
<div id="wrapper">
	<div id="menu" class="container">
		<ul>
			<li{% if true %} class="current_page_item"{% endif %}><a href="{{ path('_index') }}">{{ 'Homepage' | trans }}</a></li>
			<li{% if false %} class="current_page_item"{% endif %}><a href="#{{ path('_gallery') and false }}">{{ 'Gallery' | trans }}</a></li>
			<li{% if false %} class="current_page_item"{% endif %}><a href="#{{ path('_project') and false }}">{{ 'Project' | trans }}</a></li>
			<li{% if false %} class="current_page_item"{% endif %}><a href="#{{ path('_team') and false }}">{{ 'Team' | trans }}</a></li>
			<li{% if false %} class="current_page_item"{% endif %}><a href="#{{ path('_contact') and false }}">{{ 'Contact' | trans }}</a></li>
		</ul>
	</div>
	<!-- end #menu -->
	{% block header %}
	{% endblock %}
	<div id="page" class="container">
		<div id="content">
			{% block content %}
			{% endblock %}
		</div>
		{% block sidebar %}
		<div id="sidebar">
			<div class="box1">
				<div class="title">
					<h2>{{ 'Links' | trans }}</h2>
				</div>
				<ul class="style2">
					<li><a href="www.epitech.eu">{{ 'Epitech' | trans }}</a></li>
					<li><a href="eip.epitech.eu">{{ 'Epitech Innovative Project' | trans }}</a></li>
					<li><a href="gamedevlab.epitech.eu">{{ 'Game Dev Lab' | trans }}</a></li>
				</ul>
			</div>
